{
  "name": "sputnik-dao",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-slick": "^0.23.4",
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "svg-sprite": "^1.5.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "yarn watch-icons & react-scripts start",
    "build": "yarn build-svg-sprite & react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint --cache --max-warnings=0 src/**/*.ts{,x} && stylelint --cache -f verbose \"**/*.scss\"",
    "lint:scss": "stylelint --cache -f verbose \"**/*.scss\"",
    "watch-icons": "watch 'yarn build-svg-sprite' src/icons/",
    "build-svg-sprite": "svg-sprite -s --symbol-sprite=sprite-icons.svg --symbol-dest= --dest=public src/icons/*.svg",
    "g": "./.cli/cli g ${1} ${2}",
    "postinstall": "husky install .config/husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "css-module-types": "^0.2.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "node-sass": "4.14.1",
    "prettier": "^2.1.2",
    "react-scripts": "4.0.3",
    "stylelint": "^13.12.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "typescript": "^4.1.2",
    "typescript-plugin-css-modules": "^2.8.0"
  }
}
